{
  "name": "apollo-os-framework",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "nodemon main.js",
    "start": "node main.js",
    "lint": "eslint . --ext .js,.ts",
    "forge": "node forge/forge.js",
    "generate": "npm run forge"
  },
  "_moduleAliases": {
    "@core": "./core",
    "@config": "./config",
    "@packages": "./packages",
    "@modules": "./modules",
    "@graphql": "./graphql"
  },
  "workspaces": [
    "packages/module-utils",
    "packages/schema-utils",
    "packages/resolver-utils",
    "packages/error-utils",
    "packages/graphql-config-utils",
    "packages/plugin-utils",
    "packages/devtools",
    "packages/testing-utils",
    "packages/hooks"
  ],
  "dependencies": {
    "@apollo/server": "^4.8.0",
    "@apollo/server-plugin-landing-page-local-default": "^4.8.0",
    "@graphql-tools/schema": "^9.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "graphql-compose": "^9.1.0",
    "graphql-compose-mongoose": "^10.0.0",
    "mongoose": "^7.6.0",
    "ramda": "^0.29.1",
    "sanctuary": "^3.1.1",
    "zod": "^3.22.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "plop": "^3.1.1",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.1"
  }
}
